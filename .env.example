# OpenWeatherMap API (Free tier available at openweathermap.org)
NEXT_PUBLIC_OPENWEATHER_API_KEY=your_openweather_api_key_here

# Farmonaut Satellite Data API (Get from farmonaut.com/api)
FARMONAUT_API_KEY=your_farmonaut_api_key_here

# DJI Cloud API (For real drone telemetry)
DJI_API_BASE=https://api.dji.com/v1
DJI_API_KEY=your_dji_api_key_here

# Parrot Drone API (Alternative to DJI)
PARROT_API_BASE=https://api.parrot.com/v1
PARROT_API_KEY=your_parrot_api_key_here

# USDA NASS Agricultural Statistics API (Free, get from https://quickstats.nass.usda.gov/api)
USDA_API_KEY=your_usda_api_key_here

# EPA Pesticides API (Optional)
EPA_API_KEY=your_epa_api_key_here

# Database (for future implementation)
DATABASE_URL=postgresql://user:password@localhost:5432/agritech
POSTGRES_URL=your_vercel_postgres_url_here

# NextAuth (for future authentication)
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# Stripe (for payment processing)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
STRIPE_SECRET_KEY=your_stripe_secret_key_here

# Node Environment
NODE_ENV=development
